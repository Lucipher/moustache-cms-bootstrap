#content_inner_wrapper
  = form_for([:admin, asset_collection], :html => {:class => "site_prop", :autocomplete => "off" }) do |f|
    = render :partial => "shared/error_messages", 
             :locals => { :target => asset_collection }    
    %fieldset         
      %ul.form_fields
        %li
          = f.label :name
          = f.text_field :name 
        %li
          = f.submit button_label, :class => "submit"
          = link_to "Cancel", [:admin, :asset_collections], :class => "cancel"
        
