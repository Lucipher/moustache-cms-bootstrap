#asset_collections
  #h3 Asset Collections
  %ul
    - @asset_collections.each do |collection|
      %li{ :id => "#{collection.name}"}
        = link_to collection.name, admin_asset_collection_path(collection)
  
%div
  %ul#new_asset_collection
    %li= link_to "New Collection", new_admin_asset_collection_path