%li
  #uploader
    %p Your brower doesn't support HTML5 
:javascript
  jQuery(function() {
    return $(document).ready(function() {
      return $('#uploader').pluploadQueue({
        runtimes: 'gears,flash,html5,html4',
        url: '/admin/asset_collections/#{@asset_collection.id.to_s}/site_assets',
        multipart: true,
        multipart_params: {
          authenticity_token: '#{form_authenticity_token}' 
        }
      });
    });
  });
