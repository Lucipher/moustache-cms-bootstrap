!!!5
%html
  %head{:charset => "UTF-8"}
    %title Etherweb Admin
    = stylesheet_link_tag "admin"
    = javascript_include_tag :defaults
    = csrf_meta_tag
  %body
  - flash.each do |name, msg|
    = content_tag :div, msg, :id => "flash_#{name}"
  #wrapper
    #header
      = render :partial => "layouts/admin/nav"
    #content
      = yield