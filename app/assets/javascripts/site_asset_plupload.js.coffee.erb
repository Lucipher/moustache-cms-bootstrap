jQuery ->
  $(document).ready ->
    $('#uploader').pluploadQueue
      runtimes: 'gears,flash,html5,html4'
      unique_names: true
      url: '/admin/asset_collections/<%= #{@asset_collection.id.to_s} %>/site_assets'
      multipart: true
      multipart_params:
        authenticity_token: '<%= form_authenticity_token %>'
